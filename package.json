{
  "name": "maltem-ca-vitrine",
  "version": "2.0.4",
  "description": "Maltem's Canada official website.",
  "scripts": {
    "start": "webpack-dev-server --mode development --open src/index.jsx",
    "build": "rm -rf dist/ && webpack --mode production",
    "lint": "eslint --ext .jsx,.js src/",
    "lint:fix": "eslint --ext .jsx,.js src/ --fix",
    "tdd": "npm run test -- --watch \"src/**/*.{js,jsx}\"",
    "test": "mocha --require ignore-styles --require babel-core/register 'src/**/*.test.{js,jsx}'",
    "cover": "rm -rf coverage && NODE_ENV=test nyc npm test",
    "cover:view": "open coverage/lcov-report/index.html",
    "check": "npm audit",
    "pre-commit": "npm-run-all --parallel lint cover check"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Maltem-canada/vitrine"
  },
  "keywords": [
    "Maltem",
    "Canada",
    "Vitrine",
    "ReactJs"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Maltem-canada/vitrine/issues"
  },
  "homepage": "https://github.com/Maltem-canada/vitrine",
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.2.0",
    "css-loader": "^1.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "ignore-styles": "^5.0.1",
    "mocha": "^5.2.0",
    "nock": "^10.0.3",
    "node-sass": "^4.10.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "pre-commit": "^1.2.2",
    "proxyquire": "^2.1.0",
    "react-hot-loader": "^4.3.12",
    "sass-loader": "^7.1.0",
    "save": "^2.3.3",
    "sinon": "^7.1.1",
    "sinon-chai": "^3.3.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "uuid": "^3.3.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.12",
    "@fortawesome/free-solid-svg-icons": "^5.6.3",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "axios": "^0.18.0",
    "dotenv-extended": "^2.3.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-favicon": "0.0.14",
    "react-redux": "^5.1.1",
    "react-scripts": "^2.1.3",
    "react-slick": "^0.23.2",
    "react-typist": "^2.0.5",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "slick-carousel": "^1.8.1"
  },
  "pre-commit": "pre-commit"
}
