{
  "statements": 50,
  "branches": 50,
  "functions": 50,
  "lines": 50,
  "exclude": [
    "**/*.test.jsx",
    "**/*.test.js",
    "dist"
  ],
  "reporter": [
    "json",
    "lcov",
    "text-summary"
  ],
  "require": [
    "babel-core/register"
  ],
  "extension": [
    ".js",
    ".jsx"
  ],
  "sourceMap": true,
  "cache": false,
  "all": false,
  "instrument": true,
  "check-coverage": true,
  "report-dir": "./coverage"
}